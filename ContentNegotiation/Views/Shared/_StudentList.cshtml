@using ContentNegotiation.Models.DTO;
@model List<StudentDTO> // Define the model for the partial view

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Address</td>
            </tr>
        }
    </tbody>
</table>
